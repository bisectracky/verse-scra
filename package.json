{
  "name": "verse-scra",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "dev:debug": "vite --host --debug hmr",
    "deploy:counter:polygon": "forge script script/DeployCounter.s.sol:DeployCounter --broadcast --legacy --verify --chain-id=137 --rpc-url polygon",
    "deploy:counter:polygon:slow": "forge script script/DeployCounter.s.sol:DeployCounter --with-gas-price 99000000000 --slow --broadcast --legacy --verify --chain-id=137 --rpc-url polygon",
    "deploy:planetrouter:polygon": "forge script script/DeployPlanetRouter.s.sol:DeployPlanetRouter --broadcast --legacy --verify --chain-id=137 --rpc-url polygon",
    "deploy:planetrouter:mainnet": "forge script script/DeployPlanetRouter.s.sol:DeployPlanetRouter --broadcast --legacy --verify --chain-id=1 --rpc-url mainnet",
    "test:counter:polygon": "forge script script/DeployCounter.s.sol:DeployCounter --chain-id=137 --rpc-url polygon",
    "forge:build": "forge build",
    "forge:test": "forge test"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^5.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "thirdweb": "^5.105.41",
    "vite": "^7.1.4"
  }
}